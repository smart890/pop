import matplotlib.pyplot as plt
import numpy as np
x = np.linspace(0, 2, 100)
y = x**2
plt.plot(x, y)
plt.xlabel('x')
plt.ylabel('y')
plt.title('Plot created by Adesh Gowda')
plt.show()

